{
  "name": "ohlc",
  "version": "1.0.0",
  "scripts": {
    "test": "ava",
    "compile": "coffee -o ./ -cw coffee/",
    "watch": "npm run compile &ava -w",
    "cover": "nyc ava",
    "report": "nyc report --reporter=html",
    "update:snap": "ava --update-snapshots"
  },
  "keywords": [],
  "dependencies": {
    "lodash": "^4.17.11",
    "moment": "^2.22.2"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "coffeescript": "^2.2.3",
    "nyc": "^13.0.1"
  },
  "ava": {
    "files": [
      "test/*.js"
    ],
    "source": [
      "**/*.js"
    ],
    "failFast": false,
    "serial": false,
    "tap": false,
    "verbose": true,
    "color": true,
    "watch": false
  },
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/mick-whats/ohlc-node"
  },
  "license": "MIT",
  "author": "mick-whats"
}
